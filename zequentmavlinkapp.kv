ZequentMDBoxLayout:

<ZequentMDBoxLayout@MDBoxLayout>:
    orientation: "vertical"
    MDTopAppBar:
        title: ""
        right_action_items:
            [
            ["home", lambda x: app.callback(x), "", "Home"],
            ["message-star", lambda x: app.callback(x), "", "Message star"],
            ["message-question", lambda x: app.callback(x), "" , "Message question"],
            ["message-reply", lambda x: app.callback(x), "", "Message reply"],
            ]
        left_action_items: 
            [
            ["menu", lambda x: app.callback_1()],
            ["zequent_logo.png", lambda x: app.callback_1()]
            ]
        mode: "end"

    MDBoxLayout:
        orientation: "horizontal"
        size_hint: 1,0.1
        Button:
            text: app.get_welcome_text()
            halign: "center"
        Button:
            text: app.get_welcome_text()
            halign: "center"
        
    