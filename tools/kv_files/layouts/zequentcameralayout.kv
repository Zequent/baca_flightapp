<ZequentCameraLayout>
    id:camera_layout
    orientation: "horizontal"  
    columns: 3
    Camera:
        id: camera
        resolution: (1920, 1080)
        play: camera_switch.active
    ZequentMapView:
    MDBoxLayout:
        orientation: "horizontal"
        columns: 2
        Label:
            text: 'Camera status'
        Switch:
            id: camera_switch
            active: False