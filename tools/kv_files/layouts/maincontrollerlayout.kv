<MainControllerLayout>:
    orientation: "vertical"
    ZequentFloatLayout:
        id: zequent_float_layout
        ZequentCameraLayout:
            id: camera_layout
        ZequentLogs:
            id: row_logs
            pos: self.width,0
            size_hint: .25, .1
        ZequentLogs:
            id: fix_tele_logs
            pos: self.width*2,0
            size_hint: .25, .1
        ZequentBoxLayout:
            pos_hint: {"top":1}
            size_hint: 1,None
            height: app.root.ids.appbar.height
            spacing: "10dp"
            canvas:
                Color:
                    rgba: 0, 0, 0,.3
                Rectangle:
                    pos: self.pos
                    size: self.size
            ZequentActionIcon:
                icon: "./static/icons/language_selection.png"
                text: "test"
            ZequentActionIcon:
                icon: "./static/icons/language_selection.png"
                text: "test"
        ZequentLabel:
            size_hint: .1, None
            pos_hint:  {'right': 1, 'bottom': 1}
            font_size: dp(20)
            text: "Â© Powered by Zequent"
        ZequentBoxLayout:
            pos_hint:  {'right': 1, 'bottom': 1}
            MDFloatingActionButtonSpeedDial:
                canvas.before:
                    Color:
                        rgba: app.customColors["first"]
                hint_animation: False
                data: camera_layout.getMapViewActionsData()
                root_button_anim: True
                opacity: 1
